@model EmpDeptProject.Models.Models.Department
@{
    ViewData["Title"] = "Edit";
}



<form method="post" asp-action="Edit">
    <div class="border p-3 mt-4">
        <div class="row pb-2">
            <h2 class="text-primary">Edit Department</h2>
            <hr />

        </div>
        
        <div class="mb-3">
            <label asp-for="DeptId"></label>
            <input asp-for="DeptId" readonly class="form-control" />
            <span asp-validation-for="DeptId" class="text-danger"></span>
            </div>
            
       
        <div class="mb-3">
            <label asp-for="DeptName"></label>
            <input asp-for="DeptName" class="form-control" />
            <span asp-validation-for="DeptName" class="text-danger"></span>
           
        </div>
       
        
        <button type="submit" class="btn btn-primary" style="width:100px">Update</button>
        <a asp-controller="Departments" asp-action="Index" class="btn btn-secondary" style="width:100px">Back</a>
    </div>
</form>

@section Scripts
{
    <partial name="_ValidationScriptsPartial.cshtml" />
}

